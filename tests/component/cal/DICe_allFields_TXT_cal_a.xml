<Parameter name="COMMENTS" type="string" value="" />  
</ParameterList>
<!-- user supplied 6 parameter transform -->
<!-- this is a user supplied transform with 6 parameters seperate from the transforms determined by the camera parameters -->
<!-- can used for non-projection transformations between images - optional -->
<Parameter name="user_6_param_transform" type="string" value="{ 0, 0, 0, 0, 0, 0 }" />  
<!-- user supplied 4x4 parameter transform -->
<!-- this is a user supplied 4x4 array transform seperate from the transforms determined by the camera parameters -->
<!-- typically includes a combined rotation and translation array  - optional -->
<ParameterList name="user_4x4_param_transform">
<Parameter name="LINE 0" type="string" value="{ 1, 0, 0, 0 }" />  
<!-- R11 R12 R13 TX -->
<Parameter name="LINE 1" type="string" value="{ 0, 1, 0, 0 }" />  
<!-- R21 R22 R23 TY -->
<Parameter name="LINE 2" type="string" value="{ 0, 0, 1, 0 }" />  
<!-- R31 R32 R33 TZ -->
<Parameter name="LINE 3" type="string" value="{ 0, 0, 0, 1 }" />  
<!-- 0   0   0   1 -->
</ParameterList>
<!-- openCV rotation from from cam0 to cam 1 -->
<!-- this is the rotation matrix from openCV -->
<!-- this is the basic output from openCV stereo calibration -->
<ParameterList name="openCV_3x3_rotation_matrix">
<Parameter name="LINE 0" type="string" value="{ 1, 0, 0 }" />  
<!-- R11 R12 R13 -->
<Parameter name="LINE 1" type="string" value="{ 0, 1, 0 }" />  
<!-- R21 R22 R23 -->
<Parameter name="LINE 2" type="string" value="{ 0, 0, 1 }" />  
<!-- R31 R32 R33 -->
</ParameterList>
</ParameterList>
