<?xml version="1.0" encoding="utf-8"?>
<ParameterList>
  <!-- DICe formatted calibration file -->
  <!-- cal_file_ID must be in the file with a value of DICe_XML_Calibration_File -->
  <Parameter name="cal_file_ID" type="string" value="DICe_XML_Calibration_File" />
  <!-- type of 3D system valid field values are:  GENERIC_SYSTEM OPENCV VIC3D DICE -->
  <Parameter name="system_type_3D" type="string" value="OPENCV" />
  <!-- camera parameters (zero valued parameters don't need to be specified) -->
  <!-- the file supports up to 10 cameras 0-9 each camera is a seperate sublist of parameters -->
  <!-- the sublist must be named CAMERA {#} with {#} the number of the camera starting at 0 -->
  <!-- valid camera parameter field names are:  CX CY FX FY FS K1 K2 K3 K4 K5 K6 P1 P2 S1 S2 S3 S4 T1 T2 LD_MODEL ALPHA BETA GAMMA TX TY TZ -->
  <!-- CX,CY-image center (pix), FX,FY-pin hole distances (pix), FS-skew (deg) -->
  <!-- K1-K6-lens distortion coefficients, P1-P2-tangential distortion(openCV), S1-S4 thin prism distortion(openCV), T1,T2-Scheimpfug correction (openCV) -->
  <!-- be aware that openCV gives the values in the following order: (K1,K2,P1,P2[,K3[,K4,K5,K6[,S1,S2,S3,S4[,TX,TY]]]]) -->
  <!-- valid values for the lens distortion model are:  NONE OPENCV_DIS VIC3D_DIS K1R1_K2R2_K3R3 K1R2_K2R4_K3R6 K1R3_K2R5_K3R7 -->
  <!-- NONE no distortion model -->
  <!-- OPENCV_DIS uses the model defined in openCV 3.4.1 -->
  <!-- VIC3D_DIS uses the model defined for VIC3D -->
  <!-- K1R1_K2R2_K3R3 -> K1*R + K2*R^2 + K3*R^3 -->
  <!-- K1R2_K2R4_K3R6 -> K1*R^2 + K2*R^4 + K3*R^6 -->
  <!-- K1R3_K2R5_K3R7 -> K1*R^3 + K2*R^5 + K3*R^7 -->
  <!-- additional camera fields: -->
  <!-- CAMERA_ID: unique camera descripter, if not supplied CAMERA {#} is used -->
  <!-- IMAGE_HEIGHT_WIDTH {h, w} -->
  <!-- PIXEL_DEPTH -->
  <!-- LENS -->
  <!-- COMMENTS -->
  <!-- any parameter with a value of 0 may simply be omitted from the calibration file -->
  <ParameterList name="CAMERA 0">
    <Parameter name="CAMERA_ID" type="string" value="Camera A" />
    <Parameter name="CX" type="double" value="3.282524690221E+02" />
    <Parameter name="CY" type="double" value="2.374656698096E+02" />
    <Parameter name="FX" type="double" value="5.341694772442E+02" />
    <Parameter name="FY" type="double" value="5.288189029375E+02" />
    <Parameter name="K1" type="double" value="-2.592345048254E-01" />
    <Parameter name="K2" type="double" value="-1.273420388229E-01" />
    <Parameter name="K6" type="double" value="-4.018929670625E-01" />
    <Parameter name="LD_MODEL" type="string" value="OPENCV_DIS" />
    <Parameter name="IMAGE_HEIGHT_WIDTH" type="string" value="{ 640, 480 }" />
    <Parameter name="PIXEL_DEPTH" type="int" value="8" />
    <Parameter name="LENS" type="string" value="35mm f2" />
    <Parameter name="COMMENTS" type="string" value="camera on the left" />
  </ParameterList>
  <ParameterList name="CAMERA 1">
    <Parameter name="CAMERA_ID" type="string" value="Camera B" />
    <Parameter name="CX" type="double" value="3.138500331437E+02" />
    <Parameter name="CY" type="double" value="2.423001711052E+02" />
    <Parameter name="FX" type="double" value="5.341694772442E+02" />
    <Parameter name="FY" type="double" value="5.288189029375E+02" />
    <Parameter name="K1" type="double" value="-2.632408663590E-01" />
    <Parameter name="K2" type="double" value="-1.196920656310E-02" />
    <Parameter name="K6" type="double" value="-1.959487138239E-01" />
    <Parameter name="LD_MODEL" type="string" value="OPENCV_DIS" />
    <Parameter name="IMAGE_HEIGHT_WIDTH" type="string" value="{ 640, 480 }" />
    <Parameter name="PIXEL_DEPTH" type="int" value="8" />
    <Parameter name="LENS" type="string" value="15mm f2.2" />
    <Parameter name="COMMENTS" type="string" value="camera on the right" />
  </ParameterList>
  <!-- openCV rotation from from cam0 to cam 1 -->
  <!-- this is the rotation matrix from openCV -->
  <!-- this is the basic output from openCV stereo calibration -->
  <ParameterList name="openCV_3x3_rot_trans_matrix">
    <Parameter name="LINE 0" type="string" value="{ 9.999741331604E-01, 4.700768261799E-03, 5.443876178856E-03, -3.342839793459E+00 }" />
    <!-- R11 R12 R13 TX -->
    <Parameter name="LINE 1" type="string" value="{ -4.764218549160E-03, 9.999201838000E-01, 1.170163454318E-02, 4.670078367815E-02 }" />
    <!-- R21 R22 R23 TY -->
    <Parameter name="LINE 2" type="string" value="{ -5.388434997075E-03, -1.172726767475E-02, 9.999167145123E-01, 3.252131958135E-03 }" />
    <!-- R31 R32 R33 TZ -->
  </ParameterList>
</ParameterList>